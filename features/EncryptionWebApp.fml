    def feature WebhookSupport optional
      name "Webhook Support"
      description "Webhook notifications for encryption events"
      owner "Integration Team"
      tags "webhooks", "notifications", "events"
      safetylevel ASIL-A

  def feature Monitoring optional
    name "Monitoring and Analytics"
    description "Application monitoring, logging, and analytics"
    owner "DevOps Team"
    tags "monitoring", "analytics", "logging"
    safetylevel ASIL-A

    def feature UsageAnalytics optional
      name "Usage Analytics"
      description "Track and analyze user behavior and feature usage"
      owner "DevOps Team"
      tags "analytics", "usage-tracking", "insights"
