    def feature Favorites optional
      name "Favorite Configurations"
      description "Save and reuse preferred encryption settings"
      owner "UI/UX Team"
      tags "favorites", "settings", "convenience"
      safetylevel ASIL-A

    def feature BatchProcessing optional
      name "Batch Processing"
      description "Process multiple text inputs simultaneously"
      owner "UI/UX Team"
      tags "batch", "efficiency", "bulk-processing"
      safetylevel ASIL-A

    def feature Templates optional
      name "Encryption Templates"
      description "Predefined encryption configurations for common use cases"
      owner "UI/UX Team"
      tags "templates", "presets", "convenience"
      safetylevel ASIL-A

  def feature DataManagement optional
    name "Data Management and Export"
    description "Data handling, storage, and export capabilities"
    owner "Data Team"
    tags "data-management", "export", "storage"
    safetylevel ASIL-A

    def feature ExportFormats optional
      name "Multiple Export Formats"
      description "Export encrypted data in various formats (JSON, XML, CSV)"
      owner "Data Team"
      tags "export", "formats", "data-exchange"
      safetylevel ASIL-A

    def feature LocalStorage optional
      name "Local Storage"
      description "Store encryption preferences and history locally"
      owner "Data Team"
      tags "local-storage", "persistence", "preferences"
      safetylevel ASIL-A

    def feature DataBackup optional
      name "Data Backup and Restore"
      description "Backup and restore user data and settings"
      owner "Data Team"
      tags "backup", "restore", "data-protection"
