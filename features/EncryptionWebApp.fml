      owner "Performance Team"
      tags "caching", "performance", "optimization"
      safetylevel ASIL-A

    def feature ProgressiveEnhancement optional
      name "Progressive Enhancement"
      description "Graceful degradation for older browsers"
      owner "Performance Team"
      tags "progressive-enhancement", "compatibility", "performance"
      safetylevel ASIL-A

  def feature AdvancedEncryption optional
    name "Advanced Encryption Options"
    description "Extended encryption algorithms and configurations"
    owner "Security Team"
    tags "advanced-crypto", "algorithms", "configuration"
    safetylevel ASIL-B

    def feature MultipleAlgorithms optional
      name "Multiple Encryption Algorithms"
      description "Support for AES-128, AES-256, and ChaCha20"
      owner "Security Team"
      tags "algorithms", "aes-128", "aes-256", "chacha20"
